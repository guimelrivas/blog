<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Img url:</strong>
    <%= post.img_url %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= post.date %>
  </p>

  <p>
    <strong>Autor:</strong>
    <%= post.autor %>
  </p>

  <p>
    <strong>Comments:</strong>
    <%= post.comments %>
  </p>

</div>
